#User function Template for python3

class Solution: 
    def select(self, arr, i):
        # code here 
        for j in range(i+1,n):
            return arr[j]
    def selectionSort(self, arr,n):
        #code here
        for i in range(n-1):
            minval = i
            for j in range(i+1,n):
                if arr[minval] > arr[j]:
                    minval = j
            arr[i],arr[minval] = arr[minval],arr[i]
        return arr
